<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.ImageInput?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="600" maxWidth="800" minHeight="600" minWidth="800" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controller.levels">
    <center>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <ImageView fitHeight="600.0" fitWidth="835.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../image_source/level_background_png.png" />
                    </image>
                </ImageView>
                <Label layoutX="231.0" layoutY="171.0" text="Game Levels">
                    <font>
                        <Font name="Berlin Sans FB Demi Bold" size="60.0" />
                    </font>
                </Label>
                <Button layoutX="700.0" mnemonicParsing="false" onAction="#logout" prefHeight="50.0" prefWidth="49.0" text="Button">
                    <effect>
                        <ImageInput>
                            <source>
                                <Image url="@../../image_source/logout_logo_50.png" />
                            </source>
                        </ImageInput>
                    </effect>
                </Button>
                <Button layoutX="750.0" mnemonicParsing="false" onAction="#force_Exit" prefHeight="50.0" prefWidth="49.0" text="Button">
                    <effect>
                        <ImageInput>
                            <source>
                                <Image url="@../../image_source/logo-exit_50.png" />
                            </source>
                        </ImageInput>
                    </effect>
                </Button>
            <HBox layoutX="21.0" layoutY="292.0" prefHeight="152.0" prefWidth="766.0">
               <children>
                  <Button fx:id="button1" mnemonicParsing="false" onAction="#level1" prefHeight="152.0" prefWidth="152.0" text="Button">
                     <effect>
                        <ImageInput>
                           <source>
                              <Image url="@../../image_source/1.png" />
                           </source>
                        </ImageInput>
                     </effect>
                  </Button>
                  <Button fx:id="button2" mnemonicParsing="false" onAction="#level2" prefHeight="152.0" prefWidth="152.0" text="Button">
                     <effect>
                        <ImageInput>
                           <source>
                              <Image url="@../../image_source/2.png" />
                           </source>
                        </ImageInput>
                     </effect>
                  </Button>
                  <Button fx:id="button3" mnemonicParsing="false" onAction="#level3" prefHeight="152.0" prefWidth="152.0" text="Button">
                     <effect>
                        <ImageInput>
                           <source>
                              <Image url="@../../image_source/3.png" />
                           </source>
                        </ImageInput>
                     </effect>
                  </Button>
                  <Button fx:id="button4" mnemonicParsing="false" onAction="#level4" prefHeight="152.0" prefWidth="152.0" text="Button">
                     <effect>
                        <ImageInput>
                           <source>
                              <Image url="@../../image_source/4.png" />
                           </source>
                        </ImageInput>
                     </effect>
                  </Button>
                  <Button fx:id="button5" mnemonicParsing="false" onAction="#level5" prefHeight="152.0" prefWidth="152.0" text="Button">
                     <effect>
                        <ImageInput>
                           <source>
                              <Image url="@../../image_source/5.png" />
                           </source>
                        </ImageInput>
                     </effect>
                  </Button>
               </children>
            </HBox>
            <Button layoutX="649.0" mnemonicParsing="false" onAction="#main_menu" prefHeight="50.0" prefWidth="50.0" text="Button">
               <effect>
                  <ImageInput>
                     <source>
                        <Image url="@../../image_source/menu_logo.png" />
                     </source>
                  </ImageInput>
               </effect>
            </Button>
            <Label fx:id="error_box" alignment="CENTER" layoutY="493.0" prefHeight="58.0" prefWidth="800.0" textFill="RED">
               <font>
                  <Font name="System Bold Italic" size="40.0" />
               </font>
            </Label>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
